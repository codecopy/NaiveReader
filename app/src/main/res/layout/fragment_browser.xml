    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context="sixue.naivereader.AddBrowserFragment">

    <Button
        android:id="@+id/browse_sis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/browse_sis" />

    <Button
        android:id="@+id/browse_oldrain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/browse_oldrain" />

</LinearLayout>
